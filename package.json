{
	"name": "learn-record-front",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc -b && vite build",
		"preview": "vite preview",
		"prepare": "husky install"
	},
	"dependencies": {
		"cross": "^1.0.0",
		"element-plus": "^2.9.2",
		"eslint": "^9.17.0",
		"eslint-plugin-prettier": "^5.2.1",
		"husky": "^9.1.7",
		"lint-staged": "^15.3.0",
		"pinia": "^2.3.0",
		"prettier": "^3.4.2",
		"stylelint": "^16.12.0",
		"stylelint-prettier": "^5.0.2",
		"vue": "^3.5.13"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^5.2.1",
		"husky": "^8.0.0",
		"typescript": "~5.6.2",
		"vite": "^6.0.1",
		"vue-tsc": "^2.1.10"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js, jsx, ts, tsx, vue}": [
			"eslint --config ./eslint-config.js"
		],
		"*.{css, less, scss}": [
			"stylelint --config .stylelintrc.js"
		],
		"*.{vue, ts, tsx, js, json, html, yml, css, less, scss, md}": [
			"prettier --write"
		]
	}
}
